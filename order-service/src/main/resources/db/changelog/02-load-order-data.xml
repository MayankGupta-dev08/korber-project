<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.20.xsd">

    <changeSet id="2" author="korber">
        <loadData file="data/orders.csv"
                  tableName="orders"
                  separator=",">
            <column name="order_id" type="NUMERIC"/>
            <column name="product_id" type="NUMERIC"/>
            <column name="product_name" type="STRING"/>
            <column name="quantity" type="NUMERIC"/>
            <column name="status" type="STRING"/>
            <column name="order_date" type="DATE"/>
        </loadData>
        <!-- Reset the sequence to continue from the last loaded ID -->
        <sql dbms="h2">ALTER TABLE orders ALTER COLUMN order_id RESTART WITH 11</sql>
    </changeSet>
</databaseChangeLog>
